<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/field"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/H"
        android:layout_width="0dp"
        android:layout_height="0dp"
        layout="@layout/row8"
        app:layout_constraintBottom_toTopOf="@+id/G"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/G"
        android:layout_width="0dp"
        android:layout_height="0dp"
        layout="@layout/row7"
        app:layout_constraintBottom_toTopOf="@+id/F"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/H" />

    <include
        android:id="@+id/F"
        android:layout_width="0dp"
        android:layout_height="0dp"
        layout="@layout/row6"
        app:layout_constraintBottom_toTopOf="@+id/E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/G" />

    <include
        android:id="@+id/E"
        android:layout_width="0dp"
        android:layout_height="0dp"
        layout="@layout/row5"
        app:layout_constraintBottom_toTopOf="@+id/D"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/F" />

    <include
        android:id="@+id/D"
        android:layout_width="0dp"
        android:layout_height="0dp"
        layout="@layout/row4"
        app:layout_constraintBottom_toTopOf="@+id/C"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/E" />

    <include
        android:id="@+id/C"
        android:layout_width="0dp"
        android:layout_height="0dp"
        layout="@layout/row3"
        app:layout_constraintBottom_toTopOf="@+id/B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/D" />

    <include
        android:id="@+id/B"
        android:layout_width="0dp"
        android:layout_height="0dp"
        layout="@layout/row2"
        app:layout_constraintBottom_toTopOf="@+id/A"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/C" />

    <include
        android:id="@+id/A"
        android:layout_width="0dp"
        android:layout_height="0dp"
        layout="@layout/row1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/B" />
</androidx.constraintlayout.widget.ConstraintLayout>
